{
  "dependencyDashboard": true,
  "packageRules": [{
    "datasources": ["git-tags"],
    "registryUrls": ["github.com"]
  }],
  "regexManagers": [
    {
      "fileMatch": [".config/nvim/init.lua"],
      "matchStrings": [
        "\\({\\s*\"(?<depName>.*)\",\\s*tag = \"(?<currentValue>.*)\""
      ],
      "datasourceTemplate": "git-tags"
    }
  ]
}
